<Application x:Class="ICQ.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ICQ"
             xmlns:View="clr-namespace:ICQ.View"
             xmlns:ViewModel="clr-namespace:ICQ.ViewModel"
             StartupUri="View/MainWindow.xaml">
    
    <Application.Resources>
        
        <DataTemplate DataType="{x:Type ViewModel:MainVM}">
            <View:MainWindow></View:MainWindow>
        </DataTemplate>

        <DataTemplate DataType="{x:Type ViewModel:ChatVM}">
            <View:ChatView></View:ChatView>
        </DataTemplate>
        
    </Application.Resources>
</Application>
